			pins = "gpio0", "gpio3", "gpio22", "gpio24",
			       "gpio26", "gpio40", "gpio41", "gpio49",
			       "gpio52", "gpio91", "gpio92", "gpio95",
			       "gpio96", "gpio124", "gpio128", "gpio116",
			       "gpio122";
			function = "gpio";
			drive-strength = <16>;
			bias-pull-up;
		};
	};

	gpio_leds_default: gpio-leds-default {
		pins = "gpio7", "gpio25", "gpio87", "gpio129";
		function = "gpio";
		drive-strength = <16>;
		bias-disable;
	};

	panel_enable_active: panel-enable-active {
		pins = "gpio120";
		function = "gpio";
		drive-strength = <16>;
		bias-disable;
	};

	panel_reset_active: panel-reset-active {
		pins = "gpio6";
		function = "gpio";
		drive-strength = <16>;
		bias-disable;
	};

	hdmi_hpd: hdmi-hpd-active {
		pins = "gpio121";
		function = "gpio";
		drive-strength = <10>;
		bias-pull-up;
		input-enable;
	};
};

