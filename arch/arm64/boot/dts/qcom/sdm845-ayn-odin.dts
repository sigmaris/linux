/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/leds/common.h>
#include <dt-bindings/pinctrl/qcom,pmic-gpio.h>
#include <dt-bindings/regulator/qcom,rpmh-regulator.h>
#include <dt-bindings/sound/qcom,q6afe.h>
#include <dt-bindings/sound/qcom,q6asm.h>

#include "sdm845.dtsi"
#include "sdm845-wcd9340.dtsi"
#include "pm8998.dtsi"
#include "pmi8998.dtsi"

/ {
	model = "AYN Odin";
	compatible = "ayntec,odin", "qcom,sdm845";
	chassis-type = "tablet";

	aliases {
		serial0 = &uart9;
		hsuart0 = &uart6;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};

	battery: battery {
		compatible = "simple-battery";

		charge-full-design-microamp-hours = <3850000>;
		voltage-min-design-microvolt = <3600000>;
		voltage-max-design-microvolt = <4400000>;
	};

	connector {
		compatible = "hdmi-connector";

		label = "HDMI";
		type = "d";

		hdmi-pwr-supply = <&hdmi_en_reg>;
		hpd-gpios = <&tlmm 121 GPIO_ACTIVE_HIGH>;

		pinctrl-names = "default";
		pinctrl-0 = <&hdmi_hpd>;

		port {
			hdmi_con: endpoint {
				remote-endpoint = <&lt8912b_out>;
			};
		};
	};

	fan_en_reg: fan-en-regulator {
		compatible = "regulator-fixed";
		regulator-name = "fan_en";

		gpio = <&tlmm 44 GPIO_ACTIVE_HIGH>;
		enable-active-high;
	};

	gamepad {
		compatible = "ayntec,odin-gamepad";

		dpad-down-gpio = <&tlmm 128 GPIO_ACTIVE_LOW>;
		dpad-left-gpio = <&tlmm 52 GPIO_ACTIVE_LOW>;
		dpad-right-gpio = <&tlmm 122 GPIO_ACTIVE_LOW>;
		dpad-up-gpio = <&tlmm 124 GPIO_ACTIVE_LOW>;
		east-btn-gpio = <&tlmm 96 GPIO_ACTIVE_LOW>;
		l1-btn-gpio = <&tlmm 26 GPIO_ACTIVE_LOW>;
		home-btn-gpio = <&tlmm 49 GPIO_ACTIVE_LOW>;
		north-btn-gpio = <&tlmm 92 GPIO_ACTIVE_LOW>;
		r1-btn-gpio = <&tlmm 91 GPIO_ACTIVE_LOW>;
		rear-l-btn-gpio = <&tlmm 116 GPIO_ACTIVE_LOW>;
		rear-r-btn-gpio = <&tlmm 24 GPIO_ACTIVE_LOW>;
		select-btn-gpio = <&tlmm 0 GPIO_ACTIVE_LOW>;
		south-btn-gpio = <&tlmm 95 GPIO_ACTIVE_LOW>;
		start-btn-gpio = <&tlmm 3 GPIO_ACTIVE_LOW>;
		thumb-l-btn-gpio = <&tlmm 40 GPIO_ACTIVE_LOW>;
		thumb-r-rtn-gpio = <&tlmm 22 GPIO_ACTIVE_LOW>;
		west-btn-gpio = <&tlmm 41 GPIO_ACTIVE_LOW>;

		io-channel-names = "x-axis", "y-axis", "rx-axis",
				   "ry-axis", "l2-trigger", "r2-trigger";
		io-channels = <&pm8998_adc ADC5_GPIO3>,
			      <&pm8998_adc ADC5_GPIO2>,
			      <&pm8998_adc ADC5_GPIO5>,
			      <&pm8998_adc ADC5_GPIO6_DIV3>,
			      <&pm8998_adc ADC5_GPIO1>,
			      <&pm8998_adc ADC5_GPIO4>;
		pinctrl-names = "default";
		pinctrl-0 = <&btn_pins &btn_en_active>;

		x-axis {
			abs-range = <1110000>;
			abs-flat = <2000>;
			abs-fuzz = <2500>;
		};

		y-axis {
			abs-range = <1340000>;
			inverted;
			abs-flat = <2000>;
			abs-fuzz = <2500>;
		};

		rx-axis {
			abs-range = <1220000>;
			inverted;
			abs-flat = <2000>;
			abs-fuzz = <2500>;
		};

		ry-axis {
			abs-range = <1180000>;
			inverted;
			abs-flat = <3000>;
			abs-fuzz = <4000>;
		};

		l2-trigger {
			abs-range = <1400000>;
			inverted;
			abs-flat = <10000>;
			abs-fuzz = <10000>;
		};

		r2-trigger {
			abs-range = <1400000>;
			inverted;
			abs-flat = <10000>;
			abs-fuzz = <10000>;
		};
	};

	gpio-keys {
		compatible = "gpio-keys";

		pinctrl-names = "default";
		pinctrl-0 = <&volume_up_gpio>;

		key-volume-up {
			label = "Volume Up";
			linux,code = <KEY_VOLUMEUP>;
			gpios = <&pm8998_gpios 6 GPIO_ACTIVE_LOW>;
		};
	};

	hdmi_en_reg: hdmi-en-regulator {
		compatible = "regulator-fixed";
		regulator-name = "hdmi_en";

		gpio = <&tlmm 13 GPIO_ACTIVE_HIGH>;
		enable-active-high;
	};

	leds {
		compatible = "gpio-leds";

		pinctrl-names = "default";
		pinctrl-0 = <&gpio_leds_default>;

		led-0 {
			color = <LED_COLOR_ID_BLUE>;
			gpios = <&tlmm 7 GPIO_ACTIVE_HIGH>;
			label = "right_joystick";
		};

		led-1 {
			color = <LED_COLOR_ID_BLUE>;
			gpios = <&tlmm 25 GPIO_ACTIVE_HIGH>;
			label = "left_joystick";
		};

		led-2 {
			color = <LED_COLOR_ID_BLUE>;
			gpios = <&tlmm 87 GPIO_ACTIVE_HIGH>;
			label = "right_side";
		};

		led-3 {
			color = <LED_COLOR_ID_BLUE>;
			gpios = <&tlmm 129 GPIO_ACTIVE_HIGH>;
			label = "left_side";
		};
	};

	/*
	 * Apparently RPMh does not provide support for PM8998 S4 because it
	 * is always-on; model it as a fixed regulator.
	 */
	vreg_s4a_1p8: pm8998-smps4 {
		compatible = "regulator-fixed";
		regulator-name = "vreg_s4a_1p8";

		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;

		regulator-always-on;
		regulator-boot-on;

		vin-supply = <&vph_pwr>;
	};

	pwm_fan: pwm-fan {
		compatible = "pwm-fan";
		cooling-levels = <0 40 127>;
		#cooling-cells = <2>;
		fan-supply = <&fan_en_reg>;
		pinctrl-0 = <&pwm_fan_ctrl>;
		pinctrl-names = "default";
		// interrupt-parent = <&tlmm>;
		// interrupts = <30 IRQ_TYPE_EDGE_RISING>;
		pwms = <&pmi8998_lpg 1 50000>;
	};

	thermal-zones {
		cluster0-thermal {
			trips {
				cluster0_fan0: cluster0-fan0 {
					temperature = <50000>;
					hysteresis = <2000>;
					type = "active";
				};
				cluster0_fan1: cluster0-fan1 {
					temperature = <60000>;
					hysteresis = <2000>;
					type = "active";
				};
			};
			cooling-maps {
				map1 {
					trip = <&cluster0_fan0>;
					cooling-device = <&pwm_fan THERMAL_NO_LIMIT 1>;
				};
				map2 {
					trip = <&cluster0_fan1>;
					cooling-device = <&pwm_fan 2 THERMAL_NO_LIMIT>;
				};
			};
		};
		cluster1-thermal {
			trips {
				cluster1_fan0: cluster1-fan0 {
					temperature = <50000>;
					hysteresis = <2000>;
					type = "active";
				};
				cluster1_fan1: cluster1-fan1 {
					temperature = <60000>;
					hysteresis = <2000>;
					type = "active";
				};
			};
			cooling-maps {
				map1 {
					trip = <&cluster1_fan0>;
					cooling-device = <&pwm_fan THERMAL_NO_LIMIT 1>;
				};
				map2 {
					trip = <&cluster1_fan1>;
					cooling-device = <&pwm_fan 2 THERMAL_NO_LIMIT>;
				};
			};
		};
		gpu-top-thermal {
			trips {
				gpu_top_fan0: gpu-top-fan0 {
					temperature = <50000>;
					hysteresis = <2000>;
					type = "active";
				};
				gpu_top_fan1: gpu-top-fan1 {
					temperature = <60000>;
					hysteresis = <2000>;
					type = "active";
				};
			};
			cooling-maps {
				map1 {
					trip = <&gpu_top_fan0>;
					cooling-device = <&pwm_fan THERMAL_NO_LIMIT 1>;
				};
				map2 {
					trip = <&gpu_top_fan1>;
					cooling-device = <&pwm_fan 2 THERMAL_NO_LIMIT>;
				};
			};
		};
		gpu-bottom-thermal {
			trips {
				gpu_bottom_fan0: gpu-bottom-fan0 {
					temperature = <50000>;
					hysteresis = <2000>;
					type = "active";
				};
				gpu_bottom_fan1: gpu-bottom-fan1 {
					temperature = <60000>;
					hysteresis = <2000>;
					type = "active";
				};
			};
			cooling-maps {
				map1 {
					trip = <&gpu_bottom_fan0>;
					cooling-device = <&pwm_fan THERMAL_NO_LIMIT 1>;
				};
				map2 {
					trip = <&gpu_bottom_fan1>;
					cooling-device = <&pwm_fan 2 THERMAL_NO_LIMIT>;
				};
			};
		};
	};

	vph_pwr: vph-pwr-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vph_pwr";
		regulator-min-microvolt = <3700000>;
		regulator-max-microvolt = <3700000>;
	};
};

&adsp_pas {
	status = "okay";
	firmware-name = "qcom/sdm845/adsp.mbn";
};

&apps_rsc {
	pm8998-rpmh-regulators {
		compatible = "qcom,pm8998-rpmh-regulators";
		qcom,pmic-id = "a";

		vdd-s1-supply = <&vph_pwr>;
		vdd-s2-supply = <&vph_pwr>;
		vdd-s3-supply = <&vph_pwr>;
		vdd-s4-supply = <&vph_pwr>;
		vdd-s5-supply = <&vph_pwr>;
		vdd-s6-supply = <&vph_pwr>;
		vdd-s7-supply = <&vph_pwr>;
		vdd-s8-supply = <&vph_pwr>;
		vdd-s9-supply = <&vph_pwr>;
		vdd-s10-supply = <&vph_pwr>;
		vdd-s11-supply = <&vph_pwr>;
		vdd-s12-supply = <&vph_pwr>;
		vdd-s13-supply = <&vph_pwr>;
		vdd-l1-l27-supply = <&vreg_s7a_1p025>;
		vdd-l2-l8-l17-supply = <&vreg_s3a_1p35>;
		vdd-l3-l11-supply = <&vreg_s7a_1p025>;
		vdd-l4-l5-supply = <&vreg_s7a_1p025>;
		vdd-l6-supply = <&vph_pwr>;
		vdd-l7-l12-l14-l15-supply = <&vreg_s5a_2p04>;
		vdd-l9-supply = <&vreg_bob>;
		vdd-l10-l23-l25-supply = <&vreg_bob>;
		vdd-l13-l19-l21-supply = <&vreg_bob>;
		vdd-l16-l28-supply = <&vreg_bob>;
		vdd-l18-l22-supply = <&vreg_bob>;
		vdd-l20-l24-supply = <&vreg_bob>;
		vdd-l26-supply = <&vreg_s3a_1p35>;
		vin-lvs-1-2-supply = <&vreg_s4a_1p8>;

		vreg_s3a_1p35: smps3 {
			regulator-min-microvolt = <1352000>;
			regulator-max-microvolt = <1352000>;
		};

		vreg_s5a_2p04: smps5 {
			regulator-min-microvolt = <1904000>;
			regulator-max-microvolt = <2040000>;
		};

		vreg_s7a_1p025: smps7 {
			regulator-min-microvolt = <900000>;
			regulator-max-microvolt = <1028000>;
		};

		vdda_mipi_dsi0_pll:
		vdda_mipi_dsi1_pll:
		vdda_qlink_lv:
		vdda_ufs1_core:
		vdda_usb1_ss_core:
		vdda_qusb_hs0_1p8:
		vdda_qusb_hs0_3p1:
		vreg_l1a_0p875: ldo1 {
			regulator-min-microvolt = <880000>;
			regulator-max-microvolt = <880000>;
			regulator-initial-mode = <RPMH_REGULATOR_MODE_HPM>;
		};

		vreg_l5a_0p8: ldo5 {
			regulator-min-microvolt = <800000>;
			regulator-max-microvolt = <800000>;
			regulator-initial-mode = <RPMH_REGULATOR_MODE_HPM>;
		};

		vreg_l7a_1p8: ldo7 {
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
			regulator-initial-mode = <RPMH_REGULATOR_MODE_HPM>;
		};

		vreg_l12a_1p8: ldo12 {
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
			regulator-initial-mode = <RPMH_REGULATOR_MODE_HPM>;
		};

		vreg_l13a_2p95: ldo13 {
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <2960000>;
			regulator-initial-mode = <RPMH_REGULATOR_MODE_HPM>;
		};

		vreg_l14a_1p8: ldo14 {
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
			regulator-initial-mode = <RPMH_REGULATOR_MODE_HPM>;
		};

		vreg_l17a_1p3: ldo17 {
			regulator-min-microvolt = <1304000>;
			regulator-max-microvolt = <1304000>;
			regulator-initial-mode = <RPMH_REGULATOR_MODE_HPM>;
		};

		vreg_l20a_2p95: ldo20 {
			regulator-min-microvolt = <2950000>;
			regulator-max-microvolt = <2960000>;
			regulator-initial-mode = <RPMH_REGULATOR_MODE_HPM>;
		};

		vreg_l21a_2p95: ldo21 {
			regulator-min-microvolt = <2950000>;
			regulator-max-microvolt = <2960000>;
			regulator-initial-mode = <RPMH_REGULATOR_MODE_HPM>;
		};

		vreg_l23a_3p3: ldo23 {
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3312000>;
			regulator-initial-mode = <RPMH_REGULATOR_MODE_HPM>;
		};

		vreg_l24a_3p075: ldo24 {
			regulator-min-microvolt = <3088000>;
			regulator-max-microvolt = <3088000>;
			regulator-initial-mode = <RPMH_REGULATOR_MODE_HPM>;
		};

		vreg_l25a_3p3: ldo25 {
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3312000>;
			regulator-initial-mode = <RPMH_REGULATOR_MODE_HPM>;
		};

		vreg_l26a_1p2: ldo26 {
			regulator-min-microvolt = <1200000>;
			regulator-max-microvolt = <1200000>;
			regulator-initial-mode = <RPMH_REGULATOR_MODE_HPM>;
			regulator-boot-on;
		};

		vreg_l28a_3p0: ldo28 {
			regulator-min-microvolt = <2856000>;
			regulator-max-microvolt = <3008000>;
			regulator-initial-mode = <RPMH_REGULATOR_MODE_HPM>;
		};
	};

	pmi8998-rpmh-regulators {
		compatible = "qcom,pmi8998-rpmh-regulators";
		qcom,pmic-id = "b";

		vdd-bob-supply = <&vph_pwr>;

		vreg_bob: bob {
			regulator-min-microvolt = <3312000>;
			regulator-max-microvolt = <3600000>;
			regulator-initial-mode = <RPMH_REGULATOR_MODE_AUTO>;
			regulator-allow-bypass;
		};
	};

	pm8005-rpmh-regulators {
		compatible = "qcom,pm8005-rpmh-regulators";
		qcom,pmic-id = "c";

		vdd-s1-supply = <&vph_pwr>;
		vdd-s2-supply = <&vph_pwr>;
		vdd-s3-supply = <&vph_pwr>;
		vdd-s4-supply = <&vph_pwr>;

		vreg_s3c_0p6: smps3 {
			regulator-min-microvolt = <600000>;
			regulator-max-microvolt = <600000>;
		};
	};
};

&cdsp_pas {
	status = "okay";
	firmware-name = "qcom/sdm845/cdsp.mbn";
};

&dispcc {
	status = "okay";
};

&gcc {
	protected-clocks = <GCC_QSPI_CORE_CLK>,
			   <GCC_QSPI_CORE_CLK_SRC>,
			   <GCC_QSPI_CNOC_PERIPH_AHB_CLK>,
			   <GCC_LPASS_Q6_AXI_CLK>,
			   <GCC_LPASS_SWAY_CLK>;
};

&gmu {
	status = "okay";
};

&gpi_dma0 {
	status = "okay";
};

&gpi_dma1 {
	status = "okay";
};

&gpu {
	status = "okay";
	zap-shader {
		memory-region = <&gpu_mem>;
		firmware-name = "qcom/sdm845/a630_zap.mbn";
	};
};

&i2c4 {
	status = "okay";
	clock-frequency = <400000>;

	hdmi-bridge@48 {
		compatible = "lontium,lt8912b";
		reg = <0x48>;
		reset-gpios = <&tlmm 12 GPIO_ACTIVE_LOW>;

		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			port@0 {
				reg = <0>;

				lt8912b_in: endpoint {
					data-lanes = <0 1 2 3>;
					remote-endpoint = <&mdss_dsi1_out>;
				};
			};

			port@1 {
				reg = <1>;

				lt8912b_out: endpoint {
					remote-endpoint = <&hdmi_con>;
				};
			};
		};
	};
};

&i2c5 {
	status = "okay";
	clock-frequency = <400000>;

	synaptics-rmi4-i2c@20 {
		compatible = "syna,rmi4-i2c";
		reg = <0x20>;

		#address-cells = <1>;
		#size-cells = <0>;

		reset-gpios = <&tlmm 104 GPIO_ACTIVE_LOW>;
		interrupts-extended = <&tlmm 125 IRQ_TYPE_EDGE_FALLING>;

		vdd-supply = <&vreg_l28a_3p0>;
		vio-supply = <&vreg_l14a_1p8>;

		syna,reset-delay-ms = <200>;
		syna,startup-delay-ms = <200>;

		pinctrl-names = "default";
		pinctrl-0 = <&ts_int_active &ts_reset_active>;

		rmi4-f01@1 {
			reg = <0x01>;
			syna,nosleep-mode = <1>;
		};

		rmi4_f12: rmi4-f12@12 {
			reg = <0x12>;
			touchscreen-inverted-x;
			touchscreen-x-mm = <75>;
			touchscreen-y-mm = <132>;
			syna,sensor-type = <1>;
			syna,rezero-wait-ms = <200>;
		};
	};
};

&ibb {
	regulator-min-microvolt = <4600000>;
	regulator-max-microvolt = <6000000>;
	regulator-over-current-protection;
	regulator-pull-down;
	regulator-soft-start;
	qcom,discharge-resistor-kohms = <300>;
};

&lab {
	regulator-min-microvolt = <4600000>;
	regulator-max-microvolt = <6000000>;
	regulator-over-current-protection;
	regulator-pull-down;
	regulator-soft-start;
};

&mdss {
	status = "okay";
};

&mdss_dsi0 {
	status = "okay";
	vdda-supply = <&vreg_l26a_1p2>;

	display_panel: panel@0 {
		compatible = "innolux,td4328";
		status = "okay";
		reg = <0>;
		vddio-supply = <&vreg_l14a_1p8>;
		vddpos-supply = <&lab>;
		vddneg-supply = <&ibb>;

		#address-cells = <1>;
		#size-cells = <0>;

		backlight = <&pmi8998_wled>;
		reset-gpios = <&tlmm 6 GPIO_ACTIVE_LOW>;
		enable-gpios = <&tlmm 120 GPIO_ACTIVE_HIGH>;
		rotation = <90>;

		pinctrl-names = "default";
		pinctrl-0 = <&panel_reset_active &panel_enable_active>;

		port {
			panel_in: endpoint {
				remote-endpoint = <&mdss_dsi0_out>;
			};
		};
	};
};

&mdss_dsi0_out {
	remote-endpoint = <&panel_in>;
	data-lanes = <0 1 2 3>;
};

&mdss_dsi0_phy {
	status = "okay";
	vdds-supply = <&vdda_mipi_dsi0_pll>;
};

&mdss_dsi1 {
	vdda-supply = <&vreg_l26a_1p2>;
	status = "okay";
};

&mdss_dsi1_out {
	remote-endpoint = <&lt8912b_in>;
	data-lanes = <0 1 2 3>;
};

&mdss_dsi1_phy {
	vdds-supply = <&vreg_l1a_0p875>;
	status = "okay";
};

&mss_pil {
	status = "okay";
	firmware-name = "qcom/sdm845/mba.mbn", "qcom/sdm845/modem.mbn";
};

&pm8998_adc {
	channel@12 {
		reg = <ADC5_GPIO1>;
		label = "gpio1";
		qcom,decimation = <256>;
		qcom,pre-scaling = <1 1>;
	};

	channel@13 {
		reg = <ADC5_GPIO2>;
		label = "gpio2";
		qcom,decimation = <256>;
		qcom,pre-scaling = <1 1>;
	};

	channel@14 {
		reg = <ADC5_GPIO3>;
		label = "gpio3";
		qcom,decimation = <256>;
		qcom,pre-scaling = <1 1>;
	};

	channel@15 {
		reg = <ADC5_GPIO4>;
		label = "gpio4";
		qcom,decimation = <256>;
		qcom,pre-scaling = <1 1>;
	};

	channel@16 {
		reg = <ADC5_GPIO5>;
		label = "gpio5";
		qcom,decimation = <256>;
		qcom,pre-scaling = <1 1>;
	};

	channel@97 {
		reg = <ADC5_GPIO6_DIV3>;
		label = "gpio6";
		qcom,decimation = <256>;
		qcom,pre-scaling = <1 3>;
	};
};

