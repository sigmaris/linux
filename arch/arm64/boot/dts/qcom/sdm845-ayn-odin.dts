&tlmm {
	sdc2_default_state: sdc2-default {
		clk {
			pins = "sdc2_clk";
			bias-disable;
			drive-strength = <16>;
		};

		cmd {
			pins = "sdc2_cmd";
			bias-pull-up;
			drive-strength = <10>;
		};

		data {
			pins = "sdc2_data";
			bias-pull-up;
			drive-strength = <10>;
		};
	};

	sdc2_card_det_n: sd-card-det-n {
		pins = "gpio126";
		function = "gpio";
		bias-pull-up;
	};

	ts_int_active: ts-int-active {
		pins = "gpio125";
		function = "gpio";
		drive-strength = <16>;
		bias-pull-up;
		input-enable;
	};

	ts_reset_active: ts-reset-active {
		pins = "gpio104";
		function = "gpio";
		drive-strength = <16>;
		bias-pull-up;
	};

	btn_en_active: btn-en-active {
		pins = "gpio88";
		function = "gpio";
		drive-strength = <8>;
		bias-disable;
		output-high;
	};

	btn_pins: btn-pins {
